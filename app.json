{
  "expo": {
    "name": "react_native_dice",
    "slug": "react_native_dice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/dice.png",
    "scheme": "dice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/dice.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/dice.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/icons/dice.png",
          "resizeMode": "cover",
          "enableFullScreenImage_legacy": true
        }
      ],
    [
      "expo-font",
      {
        "fonts": [
          "./assets/fonts/PressStart2P-Regular.ttf",
          "./assets/fonts/JosefinSans-Bold.ttf",
          "./assets/fonts/JosefinSans-Light.ttf",
          "./assets/fonts/JosefinSans-Medium.ttf",
          "./assets/fonts/JosefinSans-Regular.ttf",
          "./assets/fonts/npxJosefinSans-SemiBold.ttf",
          "./assets/fonts/JosefinSans-Thin.ttf"
        ]
      }
    ]
  ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
